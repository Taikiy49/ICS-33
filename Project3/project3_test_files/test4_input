LET X1 -1
LET Y1 10
START: LET X "*"
LET Y " "
ADD X1 2
SUB Y1 1
MULT X X1
MULT Y Y1
ADD Y X
PRINT Y
GOSUB "START" IF Y1 > 0
LET C "       *****"
LET LV 0
LOOP: PRINT C
ADD LV 1
GOSUB "LOOP" IF LV <= 3
LET C "      *******"
PRINT C
LET M 3
LET N 2
GOTO 2 IF M > N
PRINT "THIS SHOULD NOT PRINT!"
PRINT "A CHRISTMAS TREE :)"
END
.